{
  "dashboard": {
    "id": null,
    "uid": null,
    "title": "${DASHBOARD_NAME}",
    "tags": ["generated"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 0,
    "panels": [
      {
        "type": "timeseries",
        "title": "HTTP Requests por Código de Status",
        "targets": [
          {
            "expr": "rate(flask_http_request_total[1m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "type": "timeseries",
        "title": "Duração Média das Requisições (s)",
        "targets": [
          {
            "expr": "rate(flask_http_request_duration_seconds_sum[1m]) / rate(flask_http_request_duration_seconds_count[1m])",
            "legendFormat": "Média por rota",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "type": "timeseries",
        "title": "Memória em Uso (Bytes)",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memória",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "type": "timeseries",
        "title": "Uso de CPU (Total em segundos)",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[1m])",
            "legendFormat": "CPU",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      }
    ]
  },
  "folderTitle": "${FOLDER_NAME}",
  "overwrite": true
}
